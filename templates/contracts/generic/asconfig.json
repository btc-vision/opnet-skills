{
    "options": {
        "transform": "@btc-vision/opnet-transform"
    },
    "targets": {
        "release": {
            "outFile": "build/MyContract.wasm",
            "optimizeLevel": 3,
            "shrinkLevel": 1,
            "converge": false,
            "noAssert": false,
            "runtime": "stub",
            "initialMemory": 1,
            "maximumMemory": 512,
            "enable": ["simd", "bulk-memory", "reference-types"]
        }
    }
}
