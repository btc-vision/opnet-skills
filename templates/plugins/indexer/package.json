{
    "name": "op20-indexer-plugin",
    "version": "1.0.0",
    "description": "OP20 Token Indexer Plugin for OPNet",
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc && npm run compile",
        "compile": "bytenode -c dist/index.js",
        "watch": "tsc --watch",
        "lint": "eslint src --ext .ts",
        "format": "prettier --write src/**/*.ts"
    },
    "dependencies": {
        "@btc-vision/plugin-sdk": "latest"
    },
    "devDependencies": {
        "typescript": "^5.9.3",
        "bytenode": "^1.5.0",
        "eslint": "latest",
        "@typescript-eslint/eslint-plugin": "latest",
        "@typescript-eslint/parser": "latest",
        "prettier": "latest"
    }
}
